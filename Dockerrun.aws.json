{
  "AWSEBDockerrunVersion": 2,
  "containerDifinitions":[
    {
      "name": "client",
      "image": "shaharboroc/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "shaharboroc/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "shaharboroc/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "shaharboroc/multi-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client","server"]

    }
  ]
}